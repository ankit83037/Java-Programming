import java.util.*;

class AsciValue{
    public static void main(String args[]){
        Scanner input = new Scanner(System.in);
        System.out.println("Enter the digits:");
        ArrayList<Integer> al = new ArrayList<>();
        for(int i=0;i<4;i++){
            al.add(input.nextInt());
        }
        
        for(Integer i: al){
            char ch = (char)(int)i;
            System.out.println(i+"-"+ch);
        }
    }
}
